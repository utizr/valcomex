{
  "name": "valcomex",
  "version": "1.0.0",
  "description": "Compact and extensible validator.",
  "author": "Viktor Marosvary",
  "contributors": [
    "Tom Verschueren <tom.verschueren@slickss.com>",
    "Aaron Sievers"
  ],

  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/coffee-script/bin/coffee --compile --output lib/ src/",
    "test": "env env=test mocha --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul test && istanbul report",
    "testQuick": "env env=test mocha --recursive --compilers coffee:coffee-script/register"
  },
  "keywords": [
    "validator",
    "object",
    "simple",
    "compact",
    "extensible"
  ],
  "dependencies": {
    "lodash": "4.13.1"
  },
  "devDependencies": {
    "coffee-coverage": "^1.0.1",
    "coffee-script": "^1.10.0",
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "mocha": "2.5.3"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/utizr/valcomex"
  },
  "bugs": {
    "url": "http://github.com/utizr/valcomex/issues"
  },
  "license": "MIT"
